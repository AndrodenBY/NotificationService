{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "NotificationService.Services.EmailService": "Debug"
    },
    "Console": {
      "FormatterName": "Systemd",
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "VirtualHostName": "/"
  },
  "EmailSettings": {
    "Host": "localhost",
    "Port": 2525,
    "EnableSsl": false,
    "DefaultUserName": "testsender@email.com",
    "SenderEmailPassword": "test",
    "Sender": "NotificationService"
  }
}
